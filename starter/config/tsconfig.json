{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@starter/*": [
        "./starter/*"
      ],
      "@slice/*": [
        "./redux/slice/*"
      ],
      "@slice": [
        "./redux/slice/index.ts"
      ],
      "@screens/*": [
        "./screens/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@hooks": [
        "./hooks"
      ]
    }
  },
}